{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6", "es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "~/*": ["./styles/*"],
      "^/*": ["./src/UsefulFunctions/*"],
      "component/*": ["./src/component/*"],
      "reducer/*": ["./src/reducer/*"],
      "Assets/*": ["./src/Assets/*"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
